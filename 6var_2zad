#include <iostream>
#include <string>
#include <cmath>


void calculation()
{
	float x = 0, y = 0, z=0, d=0, m=0;
	std::cin >> x;
	std::cin >> y;
	m = sin(atan(z/2))-sin(atan(M_PI/3));
	z = (abs(pow(exp(-x*y))+pow(10,-3)+exp(-x*y))/(M_PI+log(x*y));
	std::cout << "d" << d <<"\n";
	std::cout << "z" << z;
}
int main()
{

	calculation();
}
